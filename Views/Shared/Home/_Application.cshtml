@model IEnumerable<Applications>

<div class="container__application">
    @foreach (var item in Model)
    {
        <div class="application">
            <div class="top__appl">
                <p class="Title__block">@item.Title</p>
                <p class="Priority__block">-- @item.Priority --</p>
            </div>
            <div class="line"></div>
            <p class="Category__block">Category: <span style="font-weight: 700">@item.Category</span></p>
            <div class="line"></div>
            <div class="Desc__blocks">
                <p class="Description__block">@item.Description</p>
                <div class="likes">
                    <p class="like__title">@item.Support_like</p>
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="lucide lucide-thumbs-up"><path d="M7 10v12"/><path d="M15 5.88 14 10h5.83a2 2 0 0 1 1.92 2.56l-2.33 8A2 2 0 0 1 17.5 22H4a2 2 0 0 1-2-2v-8a2 2 0 0 1 2-2h2.76a2 2 0 0 0 1.79-1.11L12 2h0a3.13 3.13 0 0 1 3 3.88Z"/></svg>
                </div>
            </div>
        </div>
    }
</div>